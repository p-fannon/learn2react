 @media (min-width: 480px) {\n    flex-direction: row;\n    justify-content: flex-end;\n    max-height: 1000px;\n  }\n`;\n\nconst NavItem = styled.li`\n  & + & {\n    margin-top: 12px;\n  }\n\n  @media (min-width: 480px) {\n    & + & {\n      margin-top: 0;\n      margin-left: 32px;\n    }\n  }\n\n  a {\n    font-size: 16px;\n    font-weight: bold;\n    text-decoration: none;\n    color: orangered;\n    transition: color 0.25s ease-in-out;\n\n    &:hover {\n      color: blueviolet;\n    }\n  }\n`;\n\nexport default function Nav() {\n  const {aboutVisit,\n    setAboutVisit,\n    contactVisit,\n    setContactVisit,\n    homeVisit,\n    setHomeVisit,\n    portfolioVisit,\n    setPortfolioVisit} = useContext(PageVisitContext)\n  return (\n    <Header>\n      <NavWrapper>\n        <NavList>\n          <NavItem>\n            <a href=\"/\" onClick={() => setHomeVisit(homeVisit + 1)}>Home</a>\n          </NavItem>\n\n          <NavItem>\n            <a href=\"/about\" onClick={() => setAboutVisit(aboutVisit + 1)}>About</a>\n          </NavItem>\n\n          <NavItem>\n            <a href=\"/portfolio\" onClick={() => setPortfolioVisit(portfolioVisit + 1)}>Portfolio</a>\n          </NavItem>\n\n          <NavItem>\n            <a href=\"/contact\" onClick={() => setContactVisit(contactVisit + 1)}>Contact</a>\n          </NavItem>\n        </NavList>\n      </NavWrapper>\n    </Header>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"5934b943a130220e3827d12658774860","cacheData":{"env":{}}}